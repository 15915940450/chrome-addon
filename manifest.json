{
    "manifest_version": 2,
    "name": "baidu在线状态",
    "version": "1.0",
    "description": "监视baidu是否在线",
    "icons": {
        "32": "icon32.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png"
        }
    },
    "background": {
        "scripts": [
            "js/status.js"
        ]
    },
    "permissions": [
        "https://www.baidu.com/"
    ]
}
